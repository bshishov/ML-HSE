    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.svm import SVC\n",
    "\n",
    "raw = pd.read_csv('svm-data.csv', header=None)\n",
    "y = raw.loc[:,0].values\n",
    "x = raw.loc[:,1:].values\n",
    "\n",
    "clf = SVC(C=100000, random_state=241, kernel='linear')\n",
    "clf.fit(x,y)\n",
    "[i + 1 for i in clf.support_]"
   ]
